{
  "name": "travelmap",
  "version": "2.1.1",
  "type": "module",
  "scripts": {
    "geocode": "node --loader ts-node/esm src/data/main.ts",
    "w:geocode": "nodemon --watch \"src/data/**\" --ext \"ts,txt\" --ignore \"src/**/*.spec.ts\" --exec \"npm run geocode\"",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postbuild": "node post_production.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "publish": "npm run build && ngh --dir=dist/travelmap"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.1",
    "@angular/common": "^17.2.1",
    "@angular/compiler": "^17.2.1",
    "@angular/core": "^17.2.1",
    "@angular/forms": "^17.2.1",
    "@angular/platform-browser": "^17.2.1",
    "@angular/platform-browser-dynamic": "^17.2.1",
    "@angular/router": "^17.2.1",
    "@asymmetrik/ngx-leaflet": "^15.0.1",
    "@asymmetrik/ngx-leaflet-markercluster": "^17.0.0",
    "@types/leaflet.markercluster": "^1.5.4",
    "leaflet": "^1.9.3",
    "leaflet.markercluster": "^1.5.3",
    "node-geocoder": "^4.2.0",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.3.3",
    "typescript-require": "^0.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "~17.2.0",
    "@angular/compiler-cli": "^17.2.1",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet": "^1.9.8",
    "@types/node-geocoder": "^4.2.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nodemon": "^2.0.22"
  }
}
